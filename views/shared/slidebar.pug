            span Bootstrap UI
        #collapseBootstrap.collapse(aria-labelledby='headingBootstrap' data-parent='#accordionSidebar')
            .bg-white.py-2.collapse-inner.rounded
                h6.collapse-header Bootstrap UI
                a.collapse-item(href='alerts.html') Alerts
                a.collapse-item(href='buttons.html') Buttons
                a.collapse-item(href='dropdowns.html') Dropdowns
                a.collapse-item(href='modals.html') Modals
                a.collapse-item(href='popovers.html') Popovers
                a.collapse-item(href='progress-bar.html') Progress Bars
    li.nav-item
        a.nav-link.collapsed(href='#' data-toggle='collapse' data-target='#collapseForm' aria-expanded='true' aria-controls='collapseForm')
            i.fab.fa-fw.fa-wpforms
            span Post
        #collapseForm.collapse(aria-labelledby='headingForm' data-parent='#accordionSidebar')
            .bg-white.py-2.collapse-inner.rounded
                h6.collapse-header Post
                a.collapse-item(href='/admin/allpost') All Post
                a.collapse-item(href='/admin/post') New Post
    li.nav-item
        a.nav-link(href='/admin/product')
            i.fas.fa-fw.fa-palette
            span Product
    hr.sidebar-divider
    .sidebar-heading
        | Examples
    li.nav-item
        a.nav-link.collapsed(href='#' data-toggle='collapse' data-target='#collapsePage' aria-expanded='true' aria-controls='collapsePage')
            i.fas.fa-fw.fa-columns
            span Pages
        #collapsePage.collapse(aria-labelledby='headingPage' data-parent='#accordionSidebar')
            .bg-white.py-2.collapse-inner.rounded
                h6.collapse-header Example Pages
                a.collapse-item(href='login.html') Login
                a.collapse-item(href='register.html') Register
                a.collapse-item(href='404.html') 404 Page
                a.collapse-item(href='blank.html') Blank Page
    li.nav-item
        a.nav-link(href='charts.html')
            i.fas.fa-fw.fa-chart-area
            span Charts
