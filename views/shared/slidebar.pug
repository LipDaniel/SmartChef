ul#accordionSidebar.navbar-nav.sidebar.sidebar-secondary.accordion
    a.sidebar-brand.d-flex.align-items-center.justify-content-center(href='/admin')
        .sidebar-brand-icon
            img(src='/asset/img/logo/logo2.png')
        .sidebar-brand-text.mx-3 SMARTCHEF
    hr.sidebar-divider.my-0
    li.nav-item.active
        a.nav-link(href='index.html')
            i.fas.fa-fw.fa-tachometer-alt
            span Dashboard
    hr.sidebar-divider
    .sidebar-heading
        | Features
    li.nav-item
        a.nav-link.collapsed(href='#' data-toggle='collapse' data-target='#collapseBootstrap' aria-expanded='true' aria-controls='collapseBootstrap')
