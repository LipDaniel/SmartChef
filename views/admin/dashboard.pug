extends ../shared/adminlayout

block Content
    #container-wrapper.container-fluid
        .d-sm-flex.align-items-center.justify-content-between.mb-4
            h1.h3.mb-0.text-gray-800 Dashboard
            ol.breadcrumb
                li.breadcrumb-item
                    a(href='./') Home
                li.breadcrumb-item.active(aria-current='page') Dashboard
        .row.mb-3
            .col-xl-3.col-md-6.mb-4
                .card.h-100
                    .card-body
                        .row.align-items-center
                            .col.mr-2
                                .text-xs.font-weight-bold.text-uppercase.mb-1 Earnings (Monthly)
                                .h5.mb-0.font-weight-bold.text-gray-800 $40,000
                                .mt-2.mb-0.text-muted.text-xs
                                    span.text-success.mr-2
                                        i.fa.fa-arrow-up
                                        |  3.48%
                                    span Since last month
                            .col-auto
                                i.fas.fa-calendar.fa-2x.text-primary
            .col-xl-3.col-md-6.mb-4
                .card.h-100
                    .card-body
                        .row.no-gutters.align-items-center
                            .col.mr-2
                                .text-xs.font-weight-bold.text-uppercase.mb-1 Sales
                                .h5.mb-0.font-weight-bold.text-gray-800 650
                                .mt-2.mb-0.text-muted.text-xs
                                    span.text-success.mr-2
                                        i.fas.fa-arrow-up
                                        |  12%
                                    span Since last years
                            .col-auto
                                i.fas.fa-shopping-cart.fa-2x.text-success
            .col-xl-3.col-md-6.mb-4
                .card.h-100
                    .card-body
                        .row.no-gutters.align-items-center
                            .col.mr-2
                                .text-xs.font-weight-bold.text-uppercase.mb-1 New User
                                .h5.mb-0.mr-3.font-weight-bold.text-gray-800 366
                                .mt-2.mb-0.text-muted.text-xs
                                    span.text-success.mr-2
                                        i.fas.fa-arrow-up
                                        |  20.4%
                                    span Since last month
                            .col-auto
                                i.fas.fa-users.fa-2x.text-info
            .col-xl-3.col-md-6.mb-4
                .card.h-100
                    .card-body
                        .row.no-gutters.align-items-center
                            .col.mr-2
                                .text-xs.font-weight-bold.text-uppercase.mb-1 Pending Requests
                                .h5.mb-0.font-weight-bold.text-gray-800 18
                                .mt-2.mb-0.text-muted.text-xs
                                    span.text-danger.mr-2
                                        i.fas.fa-arrow-down
                                        |  1.10%
                                    span Since yesterday
                            .col-auto
                                i.fas.fa-comments.fa-2x.text-warning
            .col-xl-8.col-lg-7
                .card.mb-4
                    .card-header.py-3.d-flex.flex-row.align-items-center.justify-content-between
                        h6.m-0.font-weight-bold.text-primary Monthly Recap Report
                        .dropdown.no-arrow
                            a#dropdownMenuLink.dropdown-toggle(href='#' role='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                                i.fas.fa-ellipsis-v.fa-sm.fa-fw.text-gray-400
                            .dropdown-menu.dropdown-menu-right.shadow.animated--fade-in(aria-labelledby='dropdownMenuLink')
                                .dropdown-header Dropdown Header:
                                a.dropdown-item(href='#') Action
                                a.dropdown-item(href='#') Another action
                                .dropdown-divider
                                a.dropdown-item(href='#') Something else here
                    .card-body
                        .chart-area
                            canvas#myAreaChart
            .col-xl-4.col-lg-5
                .card.mb-4
                    .card-header.py-3.d-flex.flex-row.align-items-center.justify-content-between
                        h6.m-0.font-weight-bold.text-primary Products Sold
                        .dropdown.no-arrow
                            a#dropdownMenuLink.dropdown-toggle.btn.btn-primary.btn-sm(href='#' role='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                                | Month 
                                i.fas.fa-chevron-down
                            .dropdown-menu.dropdown-menu-right.shadow.animated--fade-in(aria-labelledby='dropdownMenuLink')
                                .dropdown-header Select Periode
                                a.dropdown-item(href='#') Today
                                a.dropdown-item(href='#') Week
                                a.dropdown-item.active(href='#') Month
                                a.dropdown-item(href='#') This Year
                    .card-body
                        .mb-3
                            .small.text-gray-500
                                | Oblong T-Shirt
                                .small.float-right
                                    b 600 of 800 Items
                            .progress(style='height: 12px;')
                                .progress-bar.bg-warning(role='progressbar' style='width: 80%' aria-valuenow='80' aria-valuemin='0' aria-valuemax='100')
                        .mb-3
                            .small.text-gray-500
                                | Gundam 90&apos;Editions
                                .small.float-right
                                    b 500 of 800 Items
                            .progress(style='height: 12px;')
                                .progress-bar.bg-success(role='progressbar' style='width: 70%' aria-valuenow='70' aria-valuemin='0' aria-valuemax='100')
                        .mb-3
                            .small.text-gray-500
                                | Rounded Hat
                                .small.float-right
                                    b 455 of 800 Items
                            .progress(style='height: 12px;')
                                .progress-bar.bg-danger(role='progressbar' style='width: 55%' aria-valuenow='55' aria-valuemin='0' aria-valuemax='100')
                        .mb-3
                            .small.text-gray-500
                                | Indomie Goreng
                                .small.float-right
                                    b 400 of 800 Items
                            .progress(style='height: 12px;')
                                .progress-bar.bg-info(role='progressbar' style='width: 50%' aria-valuenow='50' aria-valuemin='0' aria-valuemax='100')
                        .mb-3
                            .small.text-gray-500
                                | Remote Control Car Racing
                                .small.float-right
                                    b 200 of 800 Items
                            .progress(style='height: 12px;')
                                .progress-bar.bg-success(role='progressbar' style='width: 30%' aria-valuenow='30' aria-valuemin='0' aria-valuemax='100')
                    .card-footer.text-center
                        a.m-0.small.text-primary.card-link(href='#')
                            | View More 
                            i.fas.fa-chevron-right
            .col-xl-8.col-lg-7.mb-4
                .card
                    .card-header.py-3.d-flex.flex-row.align-items-center.justify-content-between
                        h6.m-0.font-weight-bold.text-primary Invoice
                        a.m-0.float-right.btn.btn-danger.btn-sm(href='#')
                            | View More 
                            i.fas.fa-chevron-right
                    .table-responsive
                        table.table.align-items-center.table-flush
                            thead.thead-light
                                tr
                                    th Order ID
                                    th Customer
                                    th Item
                                    th Status
                                    th Action
                            tbody
                                tr
                                    td
                                        a(href='#') RA0449
                                    td Udin Wayang
                                    td Nasi Padang
                                    td
                                        span.badge.badge-success Delivered
                                    td
                                        a.btn.btn-sm.btn-primary(href='#') Detail
                                tr
                                    td
                                        a(href='#') RA5324
                                    td Jaenab Bajigur
                                    td Gundam 90&apos; Edition
                                    td
                                        span.badge.badge-warning Shipping
                                    td
                                        a.btn.btn-sm.btn-primary(href='#') Detail
                                tr
                                    td
                                        a(href='#') RA8568
                                    td Rivat Mahesa
                                    td Oblong T-Shirt
